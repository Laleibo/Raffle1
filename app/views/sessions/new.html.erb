<div class="containter">
  <div class= "ticketlog">
<!-- <img src="http://www.indianaticket.com/cms/wp-content/uploads/2014/12/yellow7-300x300.jpg" alt="ticket" > -->
<h1>LOGIN</h1>

<div class= "logform">
  <%= form_tag '/login' do %>

  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>

  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="field">
  <%= submit_tag "Login" %>
</div>
<% end %>
</div>
</div>
</div>
